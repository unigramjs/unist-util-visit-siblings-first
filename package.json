{
	"name": "unist-util-visit-siblings-first",
	"description": "Breadth-first traversal for unist ASTs â€” visits siblings before descending into children",
	"version": "1.0.0-alpha.4",
	"type": "module",
	"license": "MIT",
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"private": false,
	"homepage": "https://github.com/unigramjs/unist-util-visit-siblings-first#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unigramjs/unist-util-visit-siblings-first.git"
	},
	"bugs": {
		"url": "https://github.com/unigramjs/unist-util-visit-siblings-first/issues"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"package.json"
	],
	"keywords": [
		"unist",
		"ast",
		"visit",
		"walk",
		"traverse",
		"mdast",
		"tgast",
		"hast",
		"unigramjs"
	],
	"scripts": {
		"build": "bunup",
		"dev": "bunup --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format .",
		"format:fix": "biome format --write .",
		"tsc": "tsc --noEmit",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"release": "bumpp --commit --push --tag",
		"publish:ci": "bun publish --access public --no-git-checks",
		"prepare": "simple-git-hooks"
	},
	"dependencies": {
		"@types/unist": "^3.0.3",
		"yocto-queue": "^1.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@types/bun": "^1.2.20",
		"bumpp": "^10.2.3",
		"bunup": "^0.11.2",
		"simple-git-hooks": "^2.13.1",
		"typescript": "^5.9.2"
	},
	"simple-git-hooks": {
		"pre-commit": "bun run lint:fix && git add ."
	}
}
