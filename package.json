{
	"name": "unist-util-visit-siblings-first",
	"version": "1.0.0-alpha.6",
	"description": "Breadth-first traversal for unist ASTs. Visits siblings before descending into children.",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/unigramjs/unist-util-visit-siblings-first#readme",
	"bugs": {
		"url": "https://github.com/unigramjs/unist-util-visit-siblings-first/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unigramjs/unist-util-visit-siblings-first.git"
	},
	"author": "quadratz",
	"files": [
		"dist",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"keywords": [
		"unist",
		"ast",
		"visit",
		"walk",
		"traverse",
		"mdast",
		"tgast",
		"hast",
		"unigramjs"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"devEngines": {
		"runtime": [
			{
				"name": "node",
				"version": ">=20.19",
				"onFail": "warn"
			}
		]
	},
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown",
		"check": "tsc --noEmit && biome check",
		"check:fix": "biome check --write",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"debug:test": "DEBUG='true' vitest run",
		"debug:test:watch": "DEBUG='true' vitest watch",
		"release": "changelogen --release",
		"release:github": "changelogen gh release"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@types/node": "^24.3.0",
		"@vitest/coverage-v8": "^3.2.4",
		"changelogen": "^0.6.2",
		"tsdown": "^0.14.2",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@types/unist": "^3.0.3",
		"unist-util-is": "^6.0.0"
	}
}
